// MustParse{{.enum.Name}} converts a string to a {{.enum.Name}}, and panics if is not valid.
func MustParse{{.enum.Name}}(name string) {{.enum.Name}} {
	val, err := Parse{{.enum.Name}}(name)
	if err != nil {
		panic(err)
	}
	return val
}
